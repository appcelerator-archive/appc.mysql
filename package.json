{
  "name": "appc.mysql",
  "description": "MySQL connector",
  "version": "1.4.0",
  "author": "Jeff Haynie",
  "maintainers": [
    "Jeff Haynie <jhaynie@appcelerator.com>",
    "Dawson Toth <dtoth@appcelerator.com>",
    "Chee Kiat Ng <cng@appcelerator.com"
  ],
  "license": "Apache-2.0",
  "framework": "none",
  "keywords": [
    "arrow",
    "arrow-connector",
    "appc-cli-plugin",
    "appcelerator",
    "mysql"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/appcelerator/appc.mysql"
  },
  "scripts": {
    "start": "appc run",
    "build": "npm run clean && npm run build:dev && npm run security",
    "build:dev": "npm run build:dist",
    "build:dist": "babel -d ./dist ./lib",
    "lint": "standard --fix",
    "clean": "rimraf ./tmp && rimraf dist/* coverage/*",
    "security": "retire --exitwith 0 --outputpath scan-retire.json && nsp check --output json > scan-nsp.json",
    "test": "npm run security && npm run test:unit && npm run clean",
    "test:coverage": "nyc check-coverage --lines 90 --functions 95 --branches 85 --statements 90",
    "test:unit": "tap ./test/unit/* --color --bail --coverage --coverage-report=html && npm run test:coverage",
    "test:integration": "grunt"
  },
  "private": true,
  "dependencies": {
    "async": "^0.9.0",
    "lodash": "^3.10.1",
    "mysql": "^2.9.0",
    "pkginfo": "^0.3.0",
    "semver": "^5.0.3"
  },
  "devDependencies": {
    "arrow": "^*",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.0.0",
    "babel-istanbul": "^0.12.1",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-stage-2": "^6.22.0",
    "grunt": "^0.4.5",
    "grunt-mocha-test": "^0.13.2",
    "mocha": "^1.21.4",
    "mockery": "^2.0.0",
    "nsp": "^2.6.3",
    "nyc": "^10.1.2",
    "retire": "^1.2.12",
    "rimraf": "^2.6.1",
    "should": "^4.0.4",
    "sinon": "^1.17.7",
    "standard": "^9.0.1",
    "tap": "^10.3.0"
  },
  "standard": {
    "ignore": [
      "test/integration/**"
    ]
  }
}